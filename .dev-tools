const login = async(username, password)=>{
    return await (await fetch('/api/dashboard/login',{
        method:'POST',
        body: JSON.stringify({
            username,
            password
        }),
        headers: {'Content-type': 'application/json'}})).json()
}

const logout = async(username, password)=>{
    return await (await fetch('/api/dashboard/logout',{
        method:'POST',
        headers: {'Content-type': 'application/json'}})).json()
}

const createBlogPost = async(title, details) => {
    return await (await fetch('/api/blog/posts',{
        method:'POST',
        body: JSON.stringify({
            title,
            details
        }),
        headers: {'Content-type': 'application/json'}})).json()
}